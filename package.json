  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development node server.js",
    "dev:worker": "cross-env concurrently \"pnpm run dev:worker:referal\" \"pnpm run dev:worker:listener\" --kill-others --kill-others-on-fail",
    "dev:worker:referal": "dotenv -e .env.development node workers/referal.js",
    "dev:worker:listener": "dotenv -e .env.development node workers/listener.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:worker": "cross-env NODE_ENV=production concurrently \"pnpm run start:worker:referal\" --kill-others --kill-others-on-fail",
    "start:worker:referal": "cross-env NODE_ENV=production node workers/referal.js",
    "start:worker:listener": "dotenv -e .env.production node workers/listener.js",
    "test:reff:user":"cross-env NODE_ENV=development node scripts/test-referal.js",
    "test:reff:view":"cross-env NODE_ENV=development node scripts/view-save-user-test.js",
    "db:create:dev": "cross-env NODE_ENV=development npx sequelize-cli db:create",
    "db:create:prod": "cross-env NODE_ENV=production npx sequelize-cli db:create",
    "db:migrate:dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "db:migrate:prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate"
  },
